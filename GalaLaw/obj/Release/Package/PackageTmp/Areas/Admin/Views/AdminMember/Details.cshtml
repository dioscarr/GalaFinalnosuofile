@model GalaLaw.Areas.Admin.Models.MemberModel

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Secomdary_Layout.cshtml";
}

<div class="row outer-Box" style=" margin-top:20px; padding-bottom:0!important;">
    <div class="col-md-12">
        <div class="panel panel-primary">
            <div class="panel-heading">Step <span class="badge ">1</span> <span class="label label-primary"> Select a Coountry </span> </div>
            <div class="panel-body">
                <div class="form-group " style="  padding-bottom:0!important;">
                    <div>
                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayNameFor(model => model.Member.Type)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Member.Type)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Member.FName)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Member.FName)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Member.LName)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Member.LName)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Member.MInitial)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Member.MInitial)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Member.Picture)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Member.Picture)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Member.BIO)
                            </dt>

                            <dd>
                                @Html.Raw(@Model.Member.BIO)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Member.Title)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Member.Title)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Member.Email)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Member.Email)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Member.Tel)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Member.Tel)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Member.Fax)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Member.Fax)
                            </dd>                         

                        </dl>
                    </div>
                    <p>
                        @Html.ActionLink("Edit", "Edit", new { id = Model.Member.Id }) |
                        @Html.ActionLink("Back to List", "MemberList")
                    </p>

                </div>
            </div>
        </div>
    </div>
</div>

